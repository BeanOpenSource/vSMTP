const database = csv(#{
    connector: "examples/pcre/database.db",
    access: "O_READ",
    refresh: "never",
    delimiter: ' ',
});

const pcre = [];

for record in database.get_all() {
    const pattern = record[0];
    const result = record[1];

    pcre.push(
        [regex(pattern), result]
    );
}

export pcre;
